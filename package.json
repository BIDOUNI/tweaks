{
  "name": "opti-kboudini",
  "version": "1.0.0",
  "description": "Application de tweak pour l'opti de kboudini",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Kboudini",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^25.1.0",
    "electron-builder": "^24.4.0"
  },
  "build": {
    "appId": "com.tweakkboudini.app",
    "productName": "Tweaks kboudini",
    "target": "NSIS",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    },
    "win": {
      "icon": "./img.png"
    }
  }
}
